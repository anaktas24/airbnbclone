<div class="container">

  <h2 class="text-center">My listings</h2>

  <%= link_to "Create new listing", new_listing_path, class: "btn btn-primary w-100 col-9" %>
  <hr>

  <%# Iterate over listings for current_user %>

  <div class="row">

    <% @listings.each do |listing| %>
        <div class="col-sm-6 col-md-4 col-lg-3"><%= render 'shared/card', listing: listing %></div>
    <% end  %>
  </div>
</div>
